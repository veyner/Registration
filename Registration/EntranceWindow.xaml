<Window x:Class="Registration.EntranceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Registration"
        mc:Ignorable="d"
        Title="EntranceWindow" Height="214.404" Width="376.177">
    <Grid Margin="0,0,-8,1">
        <Label x:Name="EnterNameLabel" Content="Имя пользователя" HorizontalAlignment="Left" Margin="72,53,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="EnterNameTextBox" HorizontalAlignment="Left" Height="23" Margin="188,53,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="EnterPasswordLabel" Content="Пароль" HorizontalAlignment="Left" Margin="132,84,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="EnterPasswordBox" HorizontalAlignment="Left" Margin="188,84,0,0" VerticalAlignment="Top" Width="120" Height="26"/>
        <Button x:Name="EnterButton" Content="Войти" HorizontalAlignment="Left" Margin="197,122,0,0" VerticalAlignment="Top" Width="111" Click="EnterButton_Click"/>
        <Button x:Name="GoToRegistrationButton" Content="Зарегистрироваться" HorizontalAlignment="Left" Margin="72,122,0,0" VerticalAlignment="Top" Width="120" Click="GoToRegistrationButton_Click"/>
    </Grid>
</Window>
